.sticky-wrapper{
  min-height: 100vh;
  padding-bottom: 380rpx;
}
.main{
  background: #fff;
  padding: 30rpx 0;
}
.field {
  margin: 30rpx 0;
  background: rgba(248, 248, 248, 1);
  border-radius: 8rpx;
  border: 2rpx solid rgba(170, 170, 170, 1);
}

.field .input {
  display: block;
  width: 100%;
}
.textarea{
  padding: 15rpx;
}
.title{
  font-size: 30rpx;
  font-weight: 500;
}
.btns{
  height: 350rpx;
  position: relative;
  margin-top: -350rpx;
  text-align: center;
}
.btn {
  margin: 30rpx auto;
}